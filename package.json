{
  "name": "gtfo",
  "version": "0.4.0",
  "description": "gtfo",
  "license": "MIT",
  "keywords": [
    "serverless",
    "functions",
    "api",
    "bot",
    "telegram",
    "now",
    "gtfo"
  ],
  "scripts": {
    "start:bot": "env-cmd babel-watch -w api/_bot -m \"> restarting bot...\" api/_bot/dev.js",
    "start": "parcel src/index.html --open --dist-dir public",
    "build": "parcel build src/index.html --dist-dir public",
    "clean": "rm -rf public .cache .parcel-cache",
    "prepare": "husky install"
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "lodash": "^4.17.20",
    "mathjax": "^3.1.2",
    "nerdamer": "^1.1.2",
    "p5": "^0.10.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-p5-wrapper": "^2.0.0",
    "sharp": "^0.29.1",
    "tachyons": "^4.11.1",
    "telegraf": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.8.3",
    "@parcel/packager-raw-url": "2.0.0-rc.0",
    "@parcel/service-worker": "^2.0.0-nightly.2448",
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "2.0.0-rc.0",
    "babel-watch": "^7.0.0",
    "env-cmd": "^10.0.1",
    "husky": "^7.0.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-plugin-static-files-copy": "^2.2.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
